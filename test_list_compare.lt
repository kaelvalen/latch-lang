# Test list comparison
list1 := [1, 2, 3]
list2 := [1, 2, 3]
list3 := [1, 2, 4]

# Test equality
if list1 == list2 {
    print("✓ [1,2,3] == [1,2,3] works!")
} else {
    print("✗ List equality failed")
}

# Test inequality
if list1 != list3 {
    print("✓ [1,2,3] != [1,2,4] works!")
} else {
    print("✗ List inequality failed")
}

# Test nested lists
nested1 := [[1, 2], [3, 4]]
nested2 := [[1, 2], [3, 4]]
if nested1 == nested2 {
    print("✓ Nested list comparison works!")
}

# Test dict comparison
dict1 := {"a": 1, "b": 2}
dict2 := {"a": 1, "b": 2}
if dict1 == dict2 {
    print("✓ Dict comparison works!")
}

print("All comparison tests completed!")
stop 0
