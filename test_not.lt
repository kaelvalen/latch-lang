# Test not keyword
x := true
y := false

# Test not with true
if not x {
    print("✗ not true should be false")
} else {
    print("✓ not true works!")
}

# Test not with false
if not y {
    print("✓ not false works!")
} else {
    print("✗ not false should be true")
}

# Test not with expression
z := 5
if not (z > 10) {
    print("✓ not with expression works!")
}

# Test not not (double negative)
if not not x {
    print("✓ not not true works!")
}

print("All not keyword tests completed!")
stop 0
