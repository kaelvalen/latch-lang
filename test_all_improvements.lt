# Comprehensive Test of All Latch Lang Improvements

print("=== COMPREHENSIVE LATCH LANG IMPROVEMENTS TEST ===")
print("")

# 1. elif TESTS
print("1. Testing elif...")
x := 25
if x > 50 {
    print("   ✗ elif failed")
} elif x > 20 {
    print("   ✓ elif works!")
} else {
    print("   ✗ elif failed")
}

# 2. assert() TEST
print("\n2. Testing assert()...")
assert(true, "This should pass")
print("   ✓ assert() works!")

# 3. sum() TEST
print("\n3. Testing sum()...")
nums := [1, 2, 3, 4, 5]
result := sum(nums)
assert(result == 15, "Sum should be 15")
print("   ✓ sum([1,2,3,4,5]) = ${result}")

# 4. max() TEST
print("\n4. Testing max()...")
max_val := max([3, 1, 4, 1, 5, 9, 2, 6])
assert(max_val == 9, "Max should be 9")
print("   ✓ max() = ${max_val}")

# 5. min() TEST
print("\n5. Testing min()...")
min_val := min([3, 1, 4, 1, 5, 9, 2, 6])
assert(min_val == 1, "Min should be 1")
print("   ✓ min() = ${min_val}")

# 6. repeat() TEST
print("\n6. Testing repeat()...")
repeated := repeat("ab", 3)
assert(repeated == "ababab", "Repeat failed")
print("   ✓ repeat('ab', 3) = '${repeated}'")

# 7. Division by zero TEST
print("\n7. Testing division by zero handling...")
try {
    result := 10 / 0
    print("   ✗ Division by zero not caught")
} catch e {
    print("   ✓ Division by zero properly caught!")
}

# 8. List comparison TEST
print("\n8. Testing list comparison...")
list1 := [1, 2, 3]
list2 := [1, 2, 3]
if list1 == list2 {
    print("   ✓ List comparison works!")
} else {
    print("   ✗ List comparison failed")
}

# 9. Dict comparison TEST
print("\n9. Testing dict comparison...")
dict1 := {"a": 1, "b": 2}
dict2 := {"a": 1, "b": 2}
if dict1 == dict2 {
    print("   ✓ Dict comparison works!")
} else {
    print("   ✗ Dict comparison failed")
}

# 10. not keyword TEST
print("\n10. Testing not keyword...")
flag := false
if not flag {
    print("   ✓ not keyword works!")
} else {
    print("   ✗ not keyword failed")
}

# 11. time.ms() subtraction TEST
print("\n11. Testing time.ms() subtraction...")
t1 := time.ms()
t2 := time.ms()
elapsed := t2 - t1
if elapsed >= 0 {
    print("   ✓ Time subtraction works! (${elapsed} ms)")
} else {
    print("   ✗ Time subtraction failed")
}

