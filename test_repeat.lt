# Test repeat function
result := repeat("ab", 3)
print("repeat('ab', 3) = ${result}")
assert(result == "ababab", "Repeat should work")

# Test with 0
empty := repeat("x", 0)
print("repeat('x', 0) = '${empty}'")
assert(empty == "", "Repeat with 0 should give empty string")

print("âœ“ repeat() function works!")
