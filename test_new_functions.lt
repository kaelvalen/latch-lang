# Test new built-in functions

# Test assert
print("Testing assert...")
assert(true, "This should pass")
print("✓ assert(true) passed")

# Test sum
print("\nTesting sum...")
nums := [1, 2, 3, 4, 5]
total := sum(nums)
print("sum([1,2,3,4,5]) = ${total}")
assert(total == 15, "Sum should be 15")
print("✓ sum() works")

# Test sum with floats
floats := [1.5, 2.5, 3.0]
float_sum := sum(floats)
print("sum([1.5, 2.5, 3.0]) = ${float_sum}")
assert(float_sum == 7.0, "Float sum should be 7.0")
print("✓ sum() with floats works")

# Test max
print("\nTesting max...")
max_val := max([3, 1, 4, 1, 5, 9, 2, 6])
print("max([3,1,4,1,5,9,2,6]) = ${max_val}")
assert(max_val == 9, "Max should be 9")
print("✓ max() works")

# Test max with strings
str_max := max(["apple", "banana", "cherry"])
print("max strings = ${str_max}")
print("✓ max() with strings works")

# Test min
print("\nTesting min...")
min_val := min([3, 1, 4, 1, 5, 9, 2, 6])
print("min([3,1,4,1,5,9,2,6]) = ${min_val}")
assert(min_val == 1, "Min should be 1")
print("✓ min() works")

print("\n=== All new functions work! ===")
stop 0
